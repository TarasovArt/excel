{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": false
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "no-tabs": "off",
    "linebreak-style": "off",
    "indent": "off",
    "no-undef": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}